		  "mode": "cors",
		  "credentials": "include"
		}).then(response => {
			return response.json();
		}).then(data => {
			//console.log( data );
			coin = data.user_data.coin;
			balance = Number(eval("data.wallets."+coin+".amount"));	//!!!!!!!!!!!!!!!!!!!!
			
			if(checkStartBalance == true){
				sbalance = balance;
				lastbalance = balance;
				checkStartBalance = false;
			}
			
			callback();
		})
		.catch(function(err) {
			//error catch recalls main function after (node:12244) UnhandledPromiseRejectionWarning: FetchError: invalid json response body at url
			console.log(err);
			if (run) setTimeout(() => {getGetInit()},1000);
		})
	}
	getGetInit();
}
