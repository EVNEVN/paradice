function getInit(callback) {
	function getGetInit() {
		fetch("https://api.megaplay.io/init", {
		  "headers": {
			"accept": "text/plain, */*",
			//"accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
			//"sec-ch-ua": "\" Not A;Brand\";v=\"99\", \"Chromium\";v=\"101\", \"Google Chrome\";v=\"101\"",
			//"sec-ch-ua-mobile": "?0",
			//"sec-ch-ua-platform": "\"Windows\"",
			"sec-fetch-dest": "empty",
			"sec-fetch-mode": "cors",
			"sec-fetch-site": "same-site"
		  },
		  "referrer": "https://megaplay.io/",
		  "referrerPolicy": "strict-origin-when-cross-origin",
		  "body": null,
		  "method": "POST",
		  "mode": "cors",
		  "credentials": "include"
		}).then(response => {
			return response.json();
		}).then(data => {
			//console.log( data );
			coin = data.user_data.coin;
			balance = Number(eval("data.wallets."+coin+".amount"));	//!!!!!!!!!!!!!!!!!!!!
			
			if(checkStartBalance == true){
				sbalance = balance;
				lastbalance = balance;
				checkStartBalance = false;
			}
			
			callback();
		})
		.catch(function(err) {
			//error catch recalls main function after (node:12244) UnhandledPromiseRejectionWarning: FetchError: invalid json response body at url
			console.log(err);
			if (run) setTimeout(() => {getGetInit()},1000);
		})
	}
	getGetInit();
}
